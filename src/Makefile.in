# $Id: Makefile.in,v 1.8 2001/09/27 22:37:44 fox Exp $

CC = @CC@
RC = windres
EXE=@EXE@
TARGET=atari800$(EXE)
CFLAGS=@CPPFLAGS@ @CFLAGS@
DEFS=@DEFS@

PREFIX		= @prefix@
BIN_PATH	= ${PREFIX}/bin
LIB_PATH	= ${PREFIX}/lib
MAN_PATH	= ${PREFIX}/man

INCLUDE=-I.

LDFLAGS=@LDFLAGS@
LIBS=@LIBS@

OBJ = \
	atari.o \
	cpu.o \
	monitor.o \
	sio.o \
	devices.o \
	antic.o \
	gtia.o \
	pokey.o \
	pia.o \
	cartridge.o \
	rtime.o \
	prompts.o \
	rt-config.o \
	ui.o \
	binload.o \
	list.o \
	ataripcx.o \
	log.o \
	compfile.o \
	memory.o \
	pbi.o \
	statesav.o \
	diskled.o \
	colours.o \
	pokeysnd.o \
	sndsave.o \
	cassette.o \
	input.o

OBJ += @OBJ@

all: $(TARGET)

%.o: %.c
	$(CC) -c -o $@ $(DEFS) $(INCLUDE) $(CFLAGS) $<

%.ro: %.rc
	$(RC) --define WIN32 --define __MINGW32__ --include-dir . $< $@

%.o: %.cpp
	$(CC) -c -o $@ $(DEFS) $(INCLUDE) $(CFLAGS) $<

$(TARGET): $(OBJ)
	$(CC) -o $@ $(LDFLAGS) $(OBJ) $(LIBS)

dep:
	find . -name '*.c' -printf "%P\n" \
		| xargs makedepend -f Makefile.in $(DEFS) -Y

clean:
	rm -f $(OBJ) $(TARGET)

install-svgalib : install
	chown root.root ${BIN_PATH}/$(TARGET)
	chmod 4755 ${BIN_PATH}/$(TARGET)

install :
	cp atari800 ${BIN_PATH}/$(TARGET)
	cp atari800.man ${MAN_PATH}/man1/atari800.1

        
